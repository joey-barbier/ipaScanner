# Environment Configuration for ipa.tips
ENV=production
LOG_LEVEL=info

# Application settings
ENABLE_CORS=false
MAX_UPLOAD_SIZE=500mb
DATABASE_URL=sqlite:///app/data/ipascanner.sqlite

# Analysis settings
ANALYSIS_TIMEOUT=300
CLEANUP_INTERVAL=3600

# Security (REQUIRED - Generate secure values)
SECRET_KEY=your-secret-key-here

# Optional: Custom settings
# GITHUB_TOKEN=your-github-token-for-updates